{
  "name": "gesso",
  "version": "5.0.0",
  "description": "Drupal starter theme",
  "main": "js/scripts.min.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack --config ./webpack.theme-config.js && webpack --config ./webpack.dev.js",
    "build": "webpack --config ./webpack.theme-config.js && webpack --config ./webpack.production.js",
    "storybook": "start-storybook -s ./dist -p 6006",
    "build-storybook": "npm run build && build-storybook -s ./dist -o storybook",
    "deploy-storybook": "storybook-to-ghpages --source-branch=5.x",
    "dev": "npm run start && concurrently --raw \"webpack watch --config ./webpack.dev.js\" \"npm run storybook\"",
    "component": "node lib/component.js"
  },
  "author": "Forum One",
