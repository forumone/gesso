{#
/**
 * @file
 * Theme override to display primary and secondary local tasks.
 */
#}
{% if primary %}
  {% include "@components/navigation/nav/nav--tabs/nav--tabs.twig" with {
    heading: 'Primary tabs'|t,
    modifier_class: 'nav--tabs-primary',
    tabs: primary
    } only
  %}
{% endif %}

{% if secondary %}
  {% include "@components/navigation/nav/nav--tabs/nav--tabs.twig" with {
      heading: 'Secondary tabs'|t,
      modifier_class: 'nav--tabs-secondary',
      tabs: secondary
    } only
  %}
{% endif %}
