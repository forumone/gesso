{#
/**
 * @file
 * Theme override to display primary and secondary local tasks.
 */
#}
{% if primary %}
  {% include "@components/navigation/nav/nav--tabs-primary/nav--tabs-primary.twig" with {
    tabs: primary
    } only
  %}
{% endif %}

{% if secondary %}
  {% include "@components/navigation/nav/nav--tabs-secondary/nav--tabs-secondary.twig" with {
      tabs: secondary
    } only
  %}
{% endif %}
