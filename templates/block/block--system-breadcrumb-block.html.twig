{#
 /**
  * @file
  * Theme override for the system breadcrumb block.
  */
#}

{% set classes = [
  'block',
  'block--admin',
] %}

{% set attributes = attributes.addClass(classes) %}

{% embed '@components/block/block.twig' with {
  'hide_wrapper': not logged_in,
  'has_constrain': false,
  'hide_content_wrapper': true,
  'label': false
} %}

  {% block content %}
    {{ content }}
  {% endblock %}

{% endembed %}
