{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
  ]
%}

{% set attributes = attributes.addClass(classes) %}

{% embed '@layouts/section/section.twig'
  with {
    'has_wrapper': false,
    'has_constrain': false,
    'has_content_wrapper': false,
    'label': false
  }
%}

  {% block content %}
    {{ content }}
  {% endblock %}

{% endembed %}